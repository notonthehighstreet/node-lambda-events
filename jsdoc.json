{
  "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "query": "value",
    "private": true,
    "lenient": true
  }
}
